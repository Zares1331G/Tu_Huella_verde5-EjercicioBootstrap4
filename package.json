  "license": "ISC",
  "homepage": "https://github.com/Zares1331G/Tu_Huella_verde5-EjercicioBootstrap4",
  "dependencies": {
    "concurrently": "^5.3.0",
    "popper": "^1.0.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
